
#include <iostream>
#include <cmath>

int count_primes_less_than(int m);
bool isprime(int a);

int main(void) { 
    for (n = 2; n < 2000; n = n + 1) {
        int count;
        int count = count_primes_less_than(m);
        std::out << "El numero de primos menores es:" << count << "\n";
    }
    
    return 0;
}

int count_primes_less_than(int m) {  
    int k = 0;
    for (int m = 1; m < n; m = m + 1) {
        if isprime (int m) == true {
            k = k + 1
            }
    }
    
    return k;    
}

bool isprime(int a) {
    bool b = true;
    for (int c = 2; c <= std::sqrt(a); c = c + 1) {
        if (a % c == 0) {
            b = false;
            break;
        }
    }
    
    return b;
};